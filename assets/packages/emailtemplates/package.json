{
  "name": "emailtemplates",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn run dev && yarn run prod",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service build --mode developement",
    "prod": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "watch": "vue-cli-service build --mode=developement --watch"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-theme-lark": "^13.0.1",
    "core-js": "^2.6.5",
    "he": "^1.2.0",
    "lodash": "^4.17.11",
    "mousetrap": "^1.6.2",
    "vue": "^2.6.10",
    "vue-bootstrap-toggle": "^1.1.4",
    "vue-js-modal": "^1.3.31",
    "vue-localstorage": "^0.5.0",
    "vuex": "^3.0.1",
    "vuex-persist": "^1.0.3"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-dev-utils": "^12.0.1",
    "@ckeditor/ckeditor5-dev-webpack-plugin": "^8.0.1",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "flush-promises": "^1.0.2",
    "html-loader-jest": "^0.2.1",
    "node-sass": "^4.11.0",
    "raw-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
